{% extends 'base.html.twig' %}

{% block title %}Hello GenreController!
{% endblock %}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">
		<pre>
            {{ dump(genres) }}
        </pre>
		{% for genre in genres %}
			{{genre.id}}
			{{genre.genre}}
			{{genre.lib}}
		{% endfor %}
	</div>
{% endblock %}
